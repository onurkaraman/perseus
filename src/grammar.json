{
	"lex": {
		"rules": [
			["\\s+",					"/* skip whitespace */"],
			["[0-9]+(?:\\.[0-9]+)?\\b",	"return 'NUMBER';"],
			[",",						"return ',';"],
			["\\[",						"return '[';"],
			["\\]",						"return ']';"],
			["$",						"return 'EOF';"]
		]
	},

	"bnf": {
		"file": [
			["tokens EOF",			"return $1;"]
		],
		"tokens": [
			["array",				"$$ = $1;"]
		],
		"array": [
			["[ elementlist ]",	"$$ = $2;"]
		],
		"elementlist": [
			["elementlist , number",	"$$ = $1; $1.push($3);"],
			["number",						"$$ = [$1];"]
		],
		"number": [
			["NUMBER", 						"$$ = Number(yytext);"]
		]
	}
}
